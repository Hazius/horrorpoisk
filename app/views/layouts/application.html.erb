<!-- application.html.erb -->
<!DOCTYPE html>
<html>
<head>
  	<title>horrorpoisk</title>
  	<%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  	<%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  	<%= csrf_meta_tags %>
</head>
<body>

	<div id="div_wrapper">		

		<div id="div_wrapper_top_navigation">		
			<%= render "layouts/top_navigation" %>
 		</div>

		<div id="div_wrapper_top_big_label">
			<%= render "layouts/top_big_label" %>
		</div>

		<div id="div_wrapper_select_year">
			<%= render "layouts/select_year" %>
		</div>	
	
		<div id="div_wrapper_center">

			<div id="div_wrapper_left_panel">
				<%= render "layouts/left_panel" %>
			</div>	

			<div id="div_wrapper_right_panel">
				<%= render "layouts/left_panel" %>
			</div>

			<div id="div_wrapper_yield">
				<%= yield %>
				<!--
				<div class="clear"></div>	
				<div><%= debug(params) if Rails.env.development? %></div>
				<br /><br /><br /><br />	
				-->					
 			</div> 				

 		</div>

	</div>	

</body>
</html>